<!DOCTYPE html>
<html>
<head>
  <title>Sisbase</title>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body> <!-- class=<%= params[:controller]+"_"+params[:action]%>> -->
<div class="container">

<% if user_signed_in? %>
	<% flash.each do |key, value| %>
        <div class="<%= flash_class(key) %>">
              <%= value %>
        </div>
  	<% end %>
<% end %>

<%= yield %>

</div>

</body>
</html>
